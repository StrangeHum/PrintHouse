<Page x:Class="PrintHouse.pages.CabinetPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:PrintHouse.pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="CabinetPage" Background="Beige">

    <Grid>
        <Label>Личный кабинет</Label>
        <TextBlock Text="{Binding ElementName=PersonalData, Path=FirstName}"></TextBlock>
        <TextBlock Text="{Binding ElementName=PersonalData, Path=SecondName}"></TextBlock>
    </Grid>
</Page>
